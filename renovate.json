{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "baseBranches": ["development"],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"]
    },
    {
      "matchDepTypes": ["devDependencies"]
    }
  ],
  "automerge": true 
}
