{
  "name": "@curium.rocks/openweathermap-client",
  "version": "0.305.0",
  "description": "A typescript client for the OpenWeatherMap APIs",
  "main": "build/src/lib.js",
  "scripts": {
    "test": "nyc mocha --require ts-node/register --require source-map-support/register --recursive 'test/**/*.ts'",
    "build": "node_modules/typescript/bin/tsc --project ./",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint --ext .ts src --fix",
    "clean": "rm -fr build",
    "prepare": "npm run build",
    "doc": "typedoc src/lib.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/curium-rocks/openweathermap-client.git"
  },
  "keywords": [
    "data",
    "emitter",
    "typescript",
    "owm",
    "open weather map",
    "openweathermap",
    "weather"
  ],
  "devDependencies": {
    "@types/chai": "4.3.11",
    "@types/mocha": "10.0.6",
    "@types/node": "20.10.8",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "chai": "4.3.10",
    "eslint": "8.56.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-chai-friendly": "0.7.4",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.2",
    "typedoc": "0.25.7",
    "typescript": "5.3.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov"
    ],
    "all": true
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.0.0"
  }
}
